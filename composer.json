{
    "name": "gorkalaucirica/jira-api-client",
    "description": "JIRA API client",
    "keywords": ["jira","api","library","client"],
    "homepage": "https://github.com/gorkalaucirica/JiraApiClient",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gorka Laucirica",
            "email": "gorka.lauzirika@gmail.com",
            "homepage": "http://gorkalaucirica.net/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "kriswallsmith/buzz": "0.10"
    },
    "require-dev": {
        "phpspec/phpspec": "2.0.*@dev",
        "henrikbjorn/phpspec-code-coverage" : "1.0.*@dev"
    },
    "target-dir": "GorkaLaucirica/JiraApiClient",
    "autoload": {
        "psr-0": { "GorkaLaucirica\\JiraApiClient": "" }
    },
    "autoload-dev": {
        "psr-0": { "GorkaLaucirica\\JiraApiClient\\spec\\": "spec/" }
    },
    "config": {
        "bin-dir": "bin"
    }
}